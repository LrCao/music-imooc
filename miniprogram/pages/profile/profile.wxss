page {
  background-color: #eee;
}

.profile-header {
  background-image: url(data:image/jpeg;base64,);
  background-size: cover;
  height: 480rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.8)
}

.avatar-url {
  width: 200rpx;
  height: 200rpx;
  display: block;
  overflow: hidden;
  border: 6rpx solid #fff;
  border-radius: 50%;
}

.nav {

  margin: 40rpx 0;
  background: #fff;
}

.nav-item {
  height: 100rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #ddd;
}

.iconfont {
  margin-left: 40rpx;
  color: #d43c33;
}

.text {
  margin-left: 40rpx;
}

.icon-xiangyou {
  color: #000;
  position: absolute;
  right: 60rpx;
}